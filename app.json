{
  "expo": {
    "name": "Delivera Business",
    "slug": "Delivera-Restaurants",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/notification_icon.png",
      "color": "#8ac53f",
      "androidMode": "default",
      "androidCollapsedTitle": "Delivera Business"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "restaurants.delivera.uz"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "restaurants.delivera.uz"
    }
  }
}
